<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@color/colorPrimary"/>
        </shape>
    </item>
    <item
        android:bottom="20dp"
        android:left="20dp"
        android:right="30dp"
        android:top="20dp">
        <shape>
            <solid android:color="@color/colorAccent"/>
        </shape>
    </item>
</layer-list>

