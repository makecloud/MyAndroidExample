<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oohlink.wifiprobe.everybar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.liuyihui.bar.ToolbarActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name="com.liuyihui.bar.BarDemo1Activity"></activity>
        <activity android:name="com.liuyihui.bar.SearchToolBarActivity"></activity>
        <activity android:name="com.liuyihui.bar.ParallaxStyleBarActivity"></activity>
        <activity android:name="com.liuyihui.bar.MovingToolBarActivity"></activity>
        <activity android:name="com.liuyihui.bar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.liuyihui.bar.MovingToolBarActivity2"></activity>
        <activity android:name="com.liuyihui.bar.OptionMenuExampleActivity"></activity>
    </application>

</manifest>